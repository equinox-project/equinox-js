{
  "name": "equinox-js",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@equinox-js/dynamo-store-cli": "workspace:*",
    "@vitest/coverage-v8": "^0.34.1",
    "prettier": "latest",
    "turbo": "latest",
    "vitest": "^0.34.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.18.2",
  "dependencies": {
    "typescript": "^5.2.2"
  }
}
